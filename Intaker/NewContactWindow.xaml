<Window
    x:Class="Intaker.NewContactWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Intaker"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="NewContactWindow"
    Width="300"
    Height="300"
    mc:Ignorable="d">
    <Grid>
        <StackPanel Margin="10">
            <TextBox x:Name="NameTextBox" Margin="0,0,0,5" />
            <TextBox x:Name="EmailTextBox" Margin="0,0,0,5" />
            <TextBox x:Name="PhoneNumberTextBox" Margin="0,0,0,5" />
            <Button
                x:Name="SaveButton"
                Click="SaveButton_OnClick"
                Content="Save" />
        </StackPanel>
    </Grid>
</Window>
